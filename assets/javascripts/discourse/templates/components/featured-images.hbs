<div class="images">
  {{#each featuredImageList as |t|}}
    <div class="topic-thumbnail">
      <a href='t/{{t.id}}'>
        {{preview-unbound t.thumbnails featured=featured}}
      </a>
    </div>
  {{/each}}
</div>
<div class="featured-link">
  {{discourse-tag siteSettings.topic_list_featured_images_tag}}
</div>
